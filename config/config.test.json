{
  "environment": "local",
  "logger": {
    "level": "debug"
  },
  "server": {
    "port": 3000,
    "prefix": "/v1",
    "app-key": "fake-app-key",
    "jwt-secret": "fake-jwt-secret"
  },
  "consumer": {
    "enabled": true,
    "sqs-queue-url": ""
  },
  "app": {
    "name": "go-template-test",
    "in-memory-db": false
  },
  "aws": {
    "region": "sa-east-1",
    "endpoint": "http://localhost:4566",
    "dynamodb": {
      "lead-table-name": "lead_single_table"
    }
  },
  "monitor": {
    "enabled": false,
    "new-relic": {
      "custom-event-prefix": "GoTemplateTest",
      "app-key": "nr-license-key-here",
      "endpoint": "otlp.nr-data.net:4318",
      "shutdown-timeout-seconds": 5
    }
  },
  "integration": {
    "example-api": {
      "enabled": true,
      "base-url": "https://api.example.com",
      "api-key": "fake-api-key"
    }
  }
}